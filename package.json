{
  "name": "notion-standalone",
  "version": "1.0.0",
  "description": "Electron app loading url",
  "main": "main.js",
  "scripts": {
    "start": "electron . --no-sandbox",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.electron.google",
    "productName": "Google App",
    "linux": {
      "target": "AppImage",
      "category": "Utility"
    },
    "files": [
      "main.js",
      "package.json"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.6.3",
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron"
    ]
  },
  "devDependencies": {
    "electron": "^35.1.4",
    "electron-builder": "^26.0.12"
  }
}